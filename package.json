{
  "name": "monitor",
  "productName": "monitor",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "postinstall": "install-app-deps",
    "build": "electron-packager . monitor --out=\"build\" --overwrite"
  },
  "keywords": [],
  "author": "mmawhinney",
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "monitor"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "date-fns": "^2.0.1",
    "electron-squirrel-startup": "^1.0.0",
    "knex": "^0.19.2",
    "sqlite3": "^4.1.0",
    "systeminformation": "^5.6.4"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.44",
    "@electron-forge/maker-deb": "^6.0.0-beta.44",
    "@electron-forge/maker-rpm": "^6.0.0-beta.44",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.44",
    "@electron-forge/maker-zip": "^6.0.0-beta.44",
    "electron": "6.0.3",
    "electron-builder": "^21.2.0",
    "electron-packager": "^14.0.6"
  }
}
